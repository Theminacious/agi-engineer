# GitHub App Configuration
# Register new GitHub App at: https://github.com/settings/apps/new
# Set these values in your .env file:
GITHUB_APP_ID=123456
GITHUB_APP_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----
GITHUB_CLIENT_ID=abc123def456
GITHUB_CLIENT_SECRET=client_secret_here

# Database
# Local: postgresql://user:password@localhost/agi_engineer_v2
# Docker: postgresql://agi_engineer:dev_password@db:5432/agi_engineer_v2
DATABASE_URL=postgresql://user:password@localhost/agi_engineer_v2

# API
API_HOST=0.0.0.0
API_PORT=8000
API_ENV=development

# Security
# Generate random secret keys in production:
# python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production
WEBHOOK_SECRET=your-webhook-secret-change-in-production

# AI
# Get free API key at: https://console.groq.com
GROQ_API_KEY=gsk_xxx

# Frontend
# URL where the Next.js frontend is running
FRONTEND_URL=http://localhost:3000

